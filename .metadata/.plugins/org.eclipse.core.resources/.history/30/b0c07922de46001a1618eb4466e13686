<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.board.icia.dao.IBoardDao">
	<select id="getBoardList" parameterType="Integer" resultType="board">
		<![CDATA[
		SELECT * FROM BLIST_1 WHERE RONUM >= #{pNum}*10-9 AND RONUM <= #{pNum}*10
		]]>		
	</select>
		<select id="getMemberlist" parameterType="String" resultType="member">
		SELECT M.M_ID,M.M_NAME,M.M_POINT,G.G_NAME FROM M,G WHERE M.M_POINT BETWEEN G.G_LOWPOINT AND G.G_HIGHPOINT 
	</select>
</mapper>